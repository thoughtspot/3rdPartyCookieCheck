{
  "name": "@thoughtspot/third-party-cookie-check",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --config vite.pages.config.ts",
    "build": "tsc && vite build --config vite.lib.config.ts",
    "build:lib": "tsc -p tsconfig.lib.json"
  },
  "exports": {
    ".": {
      "import": "./dist-lib/index.js"
    }
  },
  "files": [
    "dist-lib",
    "lib"
  ],
  "devDependencies": {
    "typescript": "~5.8.3",
    "vite": "^7.1.2",
    "vite-plugin-singlefile": "^2.3.0"
  },
  "dependencies": {
    "promise-postmessage": "^3.6.0"
  }
}
