{
  "name": "@thoughtspot/third-party-cookie-check",
  "version": "1.4.0",
  "type": "module",
  "description": "A library to check if third party cookies are enabled",
  "author": "ThoughtSpot",
  "license": "MIT",
  "homepage": "https://github.com/thoughtspot/third-party-cookie-check",
  "repository": {
    "type": "git",
    "url": "https://github.com/thoughtspot/third-party-cookie-check.git"
  },
  "keywords": [
    "third-party-cookie-check",
    "third-party-cookies",
    "cookies",
    "cookie-check"
  ],
  "scripts": {
    "dev": "vite --config vite.pages.config.ts",
    "build": "tsc && vite build --config vite.pages.config.ts",
    "build:lib": "tsc -p tsconfig.lib.json"
  },
  "exports": {
    ".": {
      "import": "./dist-lib/index.js"
    }
  },
  "module": "./dist-lib/index.js",
  "types": "./dist-lib/index.d.ts",
  "main": "./dist-lib/index.js",
  "files": [
    "dist-lib",
    "lib"
  ],
  "devDependencies": {
    "typescript": "~5.8.3",
    "vite": "^7.1.2",
    "vite-plugin-singlefile": "^2.3.0"
  },
  "dependencies": {
    "promise-postmessage": "^3.6.0"
  }
}
